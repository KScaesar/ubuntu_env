[mergetool "vscode"]
    cmd = code --wait $MERGED
[diff]
    tool = vscode
[difftool "vscode"]
    cmd = code --wait --diff $LOCAL $REMOTE
[credential]
    helper = store
[alias]
    ch = checkout
    br = branch
    st = status
    co = commit
    l = log --oneline --graph
[init]
	defaultBranch = main
